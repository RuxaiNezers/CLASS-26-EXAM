<h1>TO-DO LIST</h1>

<table class="table table-well">
  <thead>
    <tr>
      <th>Description</th>
      <th>completed</th>
      <!-- <th colspan="3"></th>-->
    </tr>
  </thead>

  <tbody>
    <% @todos.each do |todo| %>
      <tr>
        <td><%= todo.description %></td>
        <td><%= todo.completed %></td>

      </tr>
    <% end %>
  </tbody>
</table>
